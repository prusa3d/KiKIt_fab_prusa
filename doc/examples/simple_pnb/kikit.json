{
    "layout": {
        "hspace": "3mm",
        "vspace": "3mm",
        "rows": "4",
        "cols": "4",
        "hbackbone": "5mm",
        "vbackbone": "5mm",
        "hboneskip": 1,
        "vboneskip": 1
    },
    "tabs": {
        "type": "fixed",
        "vwidth": "2mm",
        "hwidth": "2mm"
    },
    "framing": {
        "type": "plugin",
        "code": "prusaman.Framing",
        "arg": "140mm"
    },
    "tooling": {
        "type": "plugin",
        "code": "prusaman.Tooling"
    },
    "text": {
        "type": "simple",
        "anchor": "mb",
        "voffset": "-2.5mm",
        "layer": "F.SilkS",
        "width": "2.5mm",
        "height": "2.5mm",
        "thickness": "0.5mm",
        "text": "{boardTitle} | {date} | {boardCompany} | ID: {boardId}",
        "plugin": "prusaman.Text"
    },
    "copperfill": {
        "type": "solid",
        "clearance": "3mm"
    },
    "page": {
        "anchor": "bl",
        "posx": "0mm",
        "posy": "0mm"
    },
    "post": {
        "millradius": "1mm",
        "origin": "bl"
    }
}
